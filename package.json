{
  "name": "@unicorn-ui/ui",
  "version": "0.2.0",
  "description": "A modern React component library with 131 components featuring Claymorphism, Liquid Glass, Glassmorphism, Skeuomorphism, and Minimalism",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": ["dist", "README.md", "CHANGELOG.md"],
  "scripts": {
    "dev": "concurrently \"next dev\" \"tsup --watch\"",
    "dev:docs": "next dev",
    "dev:package": "tsup --watch",
    "build": "pnpm build:package && pnpm build:docs",
    "build:package": "tsup",
    "build:docs": "next build",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "clean": "rm -rf dist .next",
    "test": "pnpm lint && pnpm typecheck && pnpm build:package",
    "create-component": "node scripts/create-component.js",
    "update-exports": "node scripts/update-exports.js",
    "verify": "node scripts/verify-build.js",
    "generate-docs": "node scripts/generate-docs.js",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build:package && changeset publish",
    "prepublishOnly": "pnpm build:package",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@types/three": "^0.182.0",
    "canvas-confetti": "^1.9.4",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cobe": "^0.6.5",
    "commander": "^12.1.0",
    "framer-motion": "^12.29.2",
    "fs-extra": "^11.3.3",
    "gsap": "^3.14.2",
    "lenis": "^1.3.17",
    "lucide-react": "^0.469.0",
    "next-themes": "^0.4.6",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "tailwind-merge": "^2.6.1",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.8",
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-a11y": "^10.2.3",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/addon-onboarding": "^10.2.3",
    "@storybook/addon-vitest": "^10.2.3",
    "@storybook/nextjs-vite": "^10.2.3",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.19.7",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "next": "^16.1.6",
    "playwright": "^1.58.1",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.3",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "bin": {
    "unicorn-ui": "./dist/cli/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "tailwind",
    "claymorphism",
    "glassmorphism",
    "liquid-glass",
    "skeuomorphism",
    "minimalism",
    "framer-motion",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Shubhjn4357/unicorn-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Shubhjn4357/unicorn-ui/issues"
  },
  "homepage": "https://github.com/Shubhjn4357/unicorn-ui#readme",
  "license": "MIT",
  "author": "Shubhjn4357",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
