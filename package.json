{
  "name": "@unicorn-ui/ui",
  "version": "1.0.0",
  "description": "A modern React component library with 131 components featuring Claymorphism, Liquid Glass, Glassmorphism, Skeuomorphism, and Minimalism",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "concurrently \"next dev\" \"tsup --watch\"",
    "dev:docs": "next dev",
    "dev:package": "tsup --watch",
    "build": "pnpm build:package && pnpm build:docs",
    "build:package": "tsup",
    "build:docs": "next build",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "clean": "rm -rf dist .next",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "pnpm lint && pnpm typecheck && pnpm test && pnpm build:package",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build:package && changeset publish",
    "prepublishOnly": "pnpm build:package",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --write"
    ]
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/three": "^0.182.0",
    "canvas-confetti": "^1.9.4",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cobe": "^0.6.5",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.29.2",
    "fs-extra": "^11.3.3",
    "gsap": "^3.14.2",
    "lenis": "^1.3.17",
    "lucide-react": "^0.469.0",
    "next-themes": "^0.4.6",
    "ora": "^8.2.0",
    "pnpm": "^10.28.2",
    "prompts": "^2.4.2",
    "react-day-picker": "^9.13.1",
    "tailwind-merge": "^2.6.1",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.8",
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-a11y": "^10.2.3",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/addon-onboarding": "^10.2.3",
    "@storybook/addon-vitest": "^10.2.3",
    "@storybook/nextjs-vite": "^10.2.3",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.19.7",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "next": "^16.1.6",
    "playwright": "^1.58.1",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.3",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "bin": {
    "unicorn-ui": "./dist/cli/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "tailwind",
    "claymorphism",
    "glassmorphism",
    "liquid-glass",
    "skeuomorphism",
    "minimalism",
    "framer-motion",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Shubhjn4357/unicorn-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Shubhjn4357/unicorn-ui/issues"
  },
  "homepage": "https://github.com/Shubhjn4357/unicorn-ui#readme",
  "license": "MIT",
  "author": "Shubhjn4357",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
